{
  "license": "MIT",
  "scripts": {
    "test": "yarn jest --verbose",
    "start": "node app/javascript/index.js"
  },
  "dependencies": {
    "@rails/webpacker": "3.5",
    "@types/chart.js": "^2.7.40",
    "@types/es6-shim": "^0.31.37",
    "@types/lodash": "^4.14.117",
    "@types/puppeteer": "^1.9.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "axios": "^0.18.0",
    "camelcase-keys": "^4.2.0",
    "chart.js": "^2.7.2",
    "chartkick": "^3.0.1",
    "flush-promises": "^1.0.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.9.0",
    "snakecase-keys": "^1.2.0",
    "ts-loader": "3.5.0",
    "ts-vue-plugin": "^0.2.0",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3",
    "vue": "^2.5.17",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "vue-jest": "^2.6.0",
    "vuex-mock-store": "^0.0.5",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "setupFiles": [
      "./jest.init.js"
    ],
    "modulePaths": [
      "app/javascript/src"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/config/",
      "<rootDir>/node_modules/",
      "<rootDir>/vendor/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && rspec spec"
    }
  }
}
